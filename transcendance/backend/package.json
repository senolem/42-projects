{
	"name": "transcendance-backend",
	"version": "1.0.0",
	"description": "transcendance by albaur, averon and faventur",
	"author": "melones",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch --require dotenv/config",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@colyseus/monitor": "^0.15.4",
		"@colyseus/playground": "^0.15.11",
		"@colyseus/tools": "^0.15.30",
		"@nestjs/axios": "^3.0.1",
		"@nestjs/common": "^10.3.0",
		"@nestjs/core": "^10.3.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.3.0",
		"@nestjs/platform-socket.io": "^10.3.0",
		"@nestjs/schedule": "^4.0.0",
		"@nestjs/sequelize": "^10.0.0",
		"@nestjs/websockets": "^10.3.0",
		"@types/express-session": "^1.17.10",
		"axios": "^1.6.5",
		"bcrypt": "^5.1.1",
		"cannon-es": "^0.20.0",
		"colyseus": "^0.15.15",
		"cookie-parser": "^1.4.6",
		"cryptr": "^6.3.0",
		"dotenv": "^16.3.2",
		"express-session": "^1.17.3",
		"fs": "^0.0.1-security",
		"jsonwebtoken": "^9.0.2",
		"otplib": "^12.0.1",
		"passport": "^0.7.0",
		"passport-42": "^1.2.6",
		"pg": "^8.11.3",
		"probe-image-size": "^7.2.3",
		"qrcode": "^1.5.3",
		"sequelize": "^6.35.2",
		"sequelize-typescript": "^2.1.6"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.3.0",
		"@nestjs/schematics": "^10.1.0",
		"@nestjs/testing": "^10.3.0",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.11",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.11.5",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"body-parser": "^1.20.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"morgan": "^1.10.0",
		"prettier": "^3.2.4",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.2",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"main": ".eslintrc.js",
	"directories": {
		"test": "test"
	}
}
